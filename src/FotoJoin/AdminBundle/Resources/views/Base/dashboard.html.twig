{% extends 'FotoJoinAdminBundle:Base:base.html.twig' %}
{% use 'FotoJoinAdminBundle:Base:navbar.html.twig' %}
{% use 'FotoJoinAdminBundle:Base:sidebar.html.twig' %}
{% use 'FotoJoinAdminBundle:Base:icons.html.twig' %}

{% set translation_domain = 'FotoJoinAdminBundle' %}
{% trans_default_domain translation_domain %}

{% block title %}{{ 'dashboard.index.title'|trans }}{% endblock %}

{% block body %}
    <div id="page-wrapper">
        <div class="row" id="page-header">
            <div class="col-lg-12">
                <h1 class="page-header">{% block icons %}{{ parent() }}{% endblock %}{{ 'dashboard.index.title'|trans }}</h1>
                {% include 'FotoJoinAdminBundle:Base:flashbag.html.twig' with { 'translation_domain': translation_domain } %}
            </div>
        </div>
        <!-- /#page-header -->

    </div>
    <!-- /#page-wrapper -->

{% endblock %}
